        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label3" HorizontalAlignment="Left" Margin="17,16,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="14" Foreground="#FF0B289D" RenderTransformOrigin="-0.175,-0.375" Height="34" Width="97" Background="Transparent" Content="Import Path : "/>
        <TextBox x:Name="txtMasterBROWSEIM" HorizontalAlignment="Left" Height="45" Margin="20,55,0,0"  VerticalAlignment="Top" Width="415" BorderBrush="#FF0B44ED" Foreground="Black" Background="White" TextWrapping="Wrap" FontSize="10"/>
        <Image x:Name="BTNMASTER_SEARCHIM" HorizontalAlignment="Right" Height="22" Margin="0,64,21.4,0" VerticalAlignment="Top" Width="25" Source="/FMSPackingList;component/Icon/folder (1).png" RenderTransformOrigin="0.461,0.41"/>
        <Button x:Name="BTNMASTER_IM" Content="Import" HorizontalAlignment="Left" Height="40" Margin="188,115,0,0" VerticalAlignment="Top" Width="100" FontSize="18"/>

    </Grid>
</Window>
