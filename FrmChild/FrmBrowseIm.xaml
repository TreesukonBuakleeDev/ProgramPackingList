        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FMSPackingList"
        mc:Ignorable="d"
        Title="Import Master Item" Height="200" Width="500" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Topmost="True">
